<tr mc:repeatable="item">
	<td style="padding:0" 0 27px;>
		<table width="100%" cellpadding="0" cellspacing="0">
			<tr>
				<td mc:edit="block_27" valign="middle" colspan="2" style="font:14px/24px" verdana, geneva, sans-serif; color:#3b434a;>
					<h2 style="text-align: center;font: 22px/30px Verdana,Geneva,sans-serif;font-weight:normal;letter-spacing:1px;color:#000;">Cars in house hold.</h2>
				</td>
			</tr>
		</table>
	</td>
</tr>
<tr mc:repeatable="item">
	<td style="padding:0 0 27px;">
		<table width="100%" cellpadding="0" cellspacing="0">
			<tr>
				<td mc:edit="block_27" valign="middle" colspan="2" style="font:14px/24px margin-bottom:20px" verdana, geneva, sans-serif; color:#3b434a;>
					All Cars
				</td>
			</tr>
		</table>
	</td>
</tr>

{% for car in cars %}
	<tr mc:repeatable="item">
		<td style="padding:0 0 27px;">
			<table width="100%" cellpadding="0" cellspacing="0">
				<tr>
					<td mc:edit="block_27" valign="middle" verdana, geneva, sans-serif; color:#3b434a;>
						<b style=" Verdana,Geneva,sans-serif;font-weight: bold; color:#aaa;">
							Year:
							{{ car.year }}
						</b>
					</td>
					<td mc:edit="block_27" valign="middle" verdana, geneva, sans-serif; color:#3b434a;>
						<b style=" Verdana,Geneva,sans-serif;font-weight: bold; color:#aaa;">
							Make:
							{{ car.make }}
						</b>
					</td>
					<td mc:edit="block_27" valign="middle" verdana, geneva, sans-serif; color:#3b434a;>
						<b style=" Verdana,Geneva,sans-serif;font-weight: bold; color:#aaa;">
							Model:
							{{ car.carModel }}
						</b>
					</td>
				</tr>
			</table>
		</td>
	</tr>
{% endfor %}
